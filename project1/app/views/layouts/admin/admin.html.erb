<!DOCTYPE html>
<html>
  <head>
    <title>Project1</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'all', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'sb-admin-2', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'select2', media: 'all', 'data-turbolinks-track': 'reload' %>
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css">
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
    
  </head>

  <body id="page-top">
      <div id="wrapper">
          <%= render 'layouts/admin/bar'%>
  
       <div id="content-wrapper" class="d-flex flex-column">
      <div id="content">
      <%= render 'layouts/admin/header'%>
      <% flash.each do |message_type, message| %>
        <div class="alert alert-<%= message_type %>"><%= message %></div>
      <% end %>
      <%= yield %>
      </div>
      <%= render 'layouts/admin/footer'%>
      </div>
    </div>
    <%= javascript_pack_tag 'multiselect', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'test', 'data-turbolinks-track': 'reload' %>
  </body>
</html>